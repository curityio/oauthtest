<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Text?>
<?import javafx.scene.web.WebView?>
<fx:root type="BorderPane" xmlns="http://javafx.com/javafx/8.0.112-ea" xmlns:fx="http://javafx.com/fxml/1">
    <top>
        <Text fx:id="text" strokeType="OUTSIDE" strokeWidth="0.0" text="Message">
            <BorderPane.margin>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
            </BorderPane.margin>
        </Text>
    </top>
    <center>
        <ScrollPane fitToHeight="true" fitToWidth="true">
            <content>
                <WebView fx:id="webView" prefHeight="-1.0" prefWidth="-1.0"/>
            </content>
        </ScrollPane>
    </center>
    <bottom>
        <Button alignment="TOP_CENTER" mnemonicParsing="false" prefHeight="27.0" prefWidth="374.0"
                text="Click here to close this window">
            <BorderPane.margin>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
            </BorderPane.margin>
        </Button>
    </bottom>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
    <opaqueInsets>
        <Insets/>
    </opaqueInsets>
</fx:root>
