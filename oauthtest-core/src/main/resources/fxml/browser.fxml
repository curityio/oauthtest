<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>
<?import javafx.scene.web.WebView?>
<fx:root alignment="TOP_CENTER" maxHeight="Infinity" spacing="20.0" type="VBox"
         xmlns="http://javafx.com/javafx/8.0.112-ea" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Text fx:id="text" strokeType="OUTSIDE" strokeWidth="0.0" text="Message"/>
        <ScrollPane>
            <content>
                <WebView fx:id="webView" prefHeight="500.0" prefWidth="500.0"/>
            </content>
        </ScrollPane>
        <Button alignment="TOP_CENTER" mnemonicParsing="false" prefHeight="27.0" prefWidth="374.0"
                text="Click here to close this window"/>
    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
</fx:root>
