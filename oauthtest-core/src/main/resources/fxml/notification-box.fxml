<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.text.Text?>
<fx:root maxHeight="Infinity" maxWidth="200.0" style="-fx-background-color: green; -fx-background-radius: 4;"
         type="GridPane" xmlns="http://javafx.com/javafx/8.0.112-ea" xmlns:fx="http://javafx.com/fxml/1">
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
    <children>
        <Text fx:id="text" strokeType="OUTSIDE" strokeWidth="0.0"
              text="Message"/>
    </children>
</fx:root>
