<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.BorderPane?>
<BorderPane xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="se.curity.oauth.core.controller.CodeFlowController">
    <center>
        <ScrollPane prefHeight="200.0" prefWidth="400.0">
            <content>
                <fx:include fx:id="authzRequestView" source="code-flow-authz-req.fxml"/>
            </content>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </padding>
            <BorderPane.margin>
                <Insets top="10.0"/>
            </BorderPane.margin>
        </ScrollPane>
    </center>
    <top>
        <fx:include fx:id="arrows" source="arrows.fxml"/>
    </top>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
</BorderPane>
